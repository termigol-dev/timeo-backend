    "module": "commonjs",
    "target": "ES2021",

    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,

    "sourceMap": true,
    "removeComments": true,

    "incremental": true,
    "skipLibCheck": true,

    "strict": false
